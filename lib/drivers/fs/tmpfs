--tmpfs driver. Works with fsd

tmpfs = {}
local oldfs = deepcopy(fs)

local files = {}

function tmpfs.list(mountPath, path)
  path = fsd.normalizePath(path)
  local r = {}
  if not files[path] then
    error("No such directory")
  end
  if not files[path].isDir then
    error("Not a directory")
  end
  for k, v in pairs(files[path]) do
    
  end
end

tmpfs = applyreadonly(tmpfs)
